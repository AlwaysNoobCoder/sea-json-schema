{
	"$id": "https://fwx.org/sea/filter.json",
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "SEA Filter JSON Schema",
	"type": "object",
	"if": {
		"properties": {
			"type": {
				"type": "string"
			}
		}
	},
	"then": {
		"$ref": "resource:/SingleFilter.json"
	},
	"else": {
		"$ref": "resource:/GroupFilter.json"
	}
}
